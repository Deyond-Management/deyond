# Deyond Wallet Environment Configuration
# Copy this file to .env and fill in your values

# ========================
# RPC Providers
# ========================
ALCHEMY_API_KEY=your_alchemy_api_key
INFURA_PROJECT_ID=your_infura_project_id

# Mainnet RPC URLs
ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}
ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}
OPTIMISM_RPC_URL=https://opt-mainnet.g.alchemy.com/v2/${ALCHEMY_API_KEY}

# Testnet RPC URLs
GOERLI_RPC_URL=https://eth-goerli.g.alchemy.com/v2/${ALCHEMY_API_KEY}
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/${ALCHEMY_API_KEY}

# ========================
# Price Data
# ========================
COINGECKO_API_KEY=your_coingecko_pro_api_key
COINGECKO_API_URL=https://pro-api.coingecko.com/api/v3

# ========================
# Error Monitoring
# ========================
SENTRY_DSN=https://your-key@sentry.io/project-id
SENTRY_ENVIRONMENT=production
SENTRY_TRACES_SAMPLE_RATE=0.2

# ========================
# Push Notifications
# ========================
EXPO_ACCESS_TOKEN=your_expo_access_token
# Firebase (for Android)
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# ========================
# Analytics
# ========================
MIXPANEL_TOKEN=your_mixpanel_token
AMPLITUDE_API_KEY=your_amplitude_api_key

# ========================
# WalletConnect
# ========================
WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# ========================
# Feature Flags
# ========================
FEATURE_FLAG_API_URL=https://your-feature-flag-service.com/api
FEATURE_FLAG_API_KEY=your_api_key

# ========================
# Backend Services
# ========================
API_BASE_URL=https://api.deyond.io
API_VERSION=v1

# ========================
# App Configuration
# ========================
APP_ENV=production
DEBUG_MODE=false
LOG_LEVEL=error
